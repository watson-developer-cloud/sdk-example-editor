{
  "name": "@watson/sdk-example-editor",
  "version": "0.3.0",
  "private": true,
  "homepage": "https://watson-developer-cloud.github.io/sdk-example-editor",
  "description": "Web editor to write SDK examples in a swagger extension",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "commit": "git-cz",
    "validate": "npm ls",
    "lint": "eslint .",
    "autofix": "eslint . --fix",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "prettier": "prettier --write \"{src,test}/**/*.{js,json}\"",
    "precommit": "lint-staged",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "IBM Corp",
  "license": "MIT",
  "engines": {
    "node": ">=12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/watson-developer-cloud/sdk-example-editor.git"
  },
  "bugs": {
    "url": "https://github.com/watson-developer-cloud/sdk-example-editor/issues"
  },
  "dependencies": {
    "@carbon/grid": "^10.3.0",
    "@carbon/icons-react": "^10.10.2",
    "@carbon/themes": "^10.11.2",
    "@reduxjs/toolkit": "^1.3.5",
    "@welldone-software/why-did-you-render": "^4.1.3",
    "carbon-components": "^10.11.2",
    "carbon-components-react": "^7.11.3",
    "carbon-icons": "^7.0.7",
    "file-saver": "^2.0.2",
    "gh-pages": "^2.2.0",
    "js-yaml": "^3.13.1",
    "jszip": "^3.4.0",
    "node-sass": "^4.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-syntax-highlighter": "^12.2.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "use-resize-observer": "^6.1.0-alpha.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run autofix",
      "git add"
    ],
    "*.css": [
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
